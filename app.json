{
  "expo": {
    "name": "Harvest_Logistics",
    "slug": "Harvest_Logistics",
    "version": "2.0.5",
    "orientation": "portrait",
    "icon": "./src/images/logistics_combine.png",
    "splash": {
      "image": "./src/images/logistics_combine.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.smartAG.HarvestLogistics",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription":"Allows App to use location services in the foreground and background.",
        "NSLocationAlwaysUsageDescription": "App uses location for navigation.",
        "NSLocationWhenInUseUsageDescription": "App uses location for navigation.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCMQli8WMDnn_349IQskisges7MMiEgqGU"
        }
    },
    "android": {
      "package": "com.smartAG.HarvestLogistics",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "web": {
      "favicon": "./src/images/logistics_combine.png"
    },
    "plugins": [
      "sentry-expo"
    ]
  }
}
